<p><%= link_to 'Orders', '#', id: 'order_list' %></p>

<div id="orders">
  <table>
    <tr>
      <th>Order Number</th>
      <th>Customer</th>
      <th>Date Created</th>
    </tr>
    <% @orders.each do |order| %>
      <td><%= order.id %></td>
      <td><%= order.customer.first_name %></td>
      <td><%= order.created_at %></td>
    <% end %>
  </table>
</div>
